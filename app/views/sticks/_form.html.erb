<% if @stick.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@stick.errors.count, "error") %>
      prohibited this post from being saved:
    </h2>
 
    <ul>
    <% @stick.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>




<%= form_for @stick do |f| %>
    <%= f.label :brand %>
    <div><%= f.text_field :brand %></div>
    <p><%= f.label :model %></p>
    <div><%= f.text_field :model %></div>
    <p><%= f.label :size %></p>
    <div><%= f.text_field :size %></div>
    <p><%= f.label :flex %></p>
    <div><%= f.text_field :flex %></div>
  
     <%= f.hidden_field :user_id, value: current_user.id %>

    <%= f.submit "Submit" %>
<% end %>